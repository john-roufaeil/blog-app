{
    "version": 2,
    "builds": [
        {
            "src": "pages/api/**/*.ts",
            "use": "@vercel/node"
        },
        {
            "src": "next.config.js",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/graphql",
            "dest": "/api/graphql.ts"
        }
    ]
}